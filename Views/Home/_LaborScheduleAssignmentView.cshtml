                @*<div class="EmployeeScheduler" style="padding-left: 20px">
                        End:
                        @Html.DropDownListFor(m => m.ThisWeek.endHour, new SelectList(Model.ThisWeek.ScheduleEndHours, "Value", "Key"), new { id = "endHour" })
